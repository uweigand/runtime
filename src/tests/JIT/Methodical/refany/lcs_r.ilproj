<Project Sdk="Microsoft.NET.Sdk.IL">
  <PropertyGroup>
    <!-- Needed for CLRTestTargetUnsupported -->
    <RequiresProcessIsolation>true</RequiresProcessIsolation>
    <!-- Interpreter behaves like Mono, see issue #34196 -->
    <CLRTestTargetUnsupported Condition="'$(TargetArchitecture)' == 's390x'">true</CLRTestTargetUnsupported>
  </PropertyGroup>
  <PropertyGroup>
    <DebugType>PdbOnly</DebugType>
  </PropertyGroup>
  <ItemGroup>
    <Compile Include="lcs_r.il" />
    <Compile Include="lcs.il" />
  </ItemGroup>
</Project>

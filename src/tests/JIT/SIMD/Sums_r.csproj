<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <!-- Needed for CLRTestTargetUnsupported -->
    <RequiresProcessIsolation>true</RequiresProcessIsolation>
    <!-- Test runs too long in interpreter-only mode -->
    <CLRTestTargetUnsupported Condition="'$(TargetArchitecture)' == 's390x'">true</CLRTestTargetUnsupported>
  </PropertyGroup>
  <PropertyGroup>
    <DebugType>None</DebugType>
    <Optimize />
  </PropertyGroup>
  <ItemGroup>
    <Compile Include="Sums.cs" />
  </ItemGroup>
</Project>

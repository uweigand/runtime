<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <!-- Needed for CLRTestTargetUnsupported -->
    <RequiresProcessIsolation>true</RequiresProcessIsolation>
    <!-- Missing intrinsic support in CoreCLR interpreter -->
    <CLRTestTargetUnsupported Condition="'$(TargetArchitecture)' == 's390x'">true</CLRTestTargetUnsupported>
  </PropertyGroup>
  <PropertyGroup>
    <DebugType>None</DebugType>
    <Optimize />
  </PropertyGroup>
  <ItemGroup>
    <Compile Include="Matrix4x4.cs" />
  </ItemGroup>
</Project>

  <ItemGroup>
    <Compile Include="MethodImplOptionsTests.cs" />
  </ItemGroup>
  <ItemGroup>
    <None Include="MiAggressiveOptimization.il">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </None>
  </ItemGroup>
</Project>
